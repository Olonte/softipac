<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" 
	updateCheck="true"
	monitoring="autodetect" 
	dynamicConfig="true">

<diskStore path="java.io.tmpdir" />
	
	 <defaultCache 
	 	maxElementsInMemory="10000" 
	 	eternal="true"
        overflowToDisk="false" diskPersistent="false"
        diskExpiryThreadIntervalSeconds="1800" 
        memoryStoreEvictionPolicy="FIFO">
    </defaultCache>
    
    <cache name="tiposUsuario" 
		maxEntriesLocalHeap="100"
		maxEntriesLocalDisk="1000" 
		eternal="false" 
		timeToIdleSeconds="300" 
		timeToLiveSeconds="600">
		<persistence strategy="localTempSwap" />
	</cache>
    
    <cache name="tipoUsuarioPorId" 
		maxEntriesLocalHeap="100"
		maxEntriesLocalDisk="1000" 
		eternal="false" 
		timeToIdleSeconds="300" 
		timeToLiveSeconds="600">
		<persistence strategy="localTempSwap" />
	</cache>
    
    <cache name="estados" 
		maxEntriesLocalHeap="100"
		maxEntriesLocalDisk="1000" 
		eternal="false" 
		timeToIdleSeconds="300" 
		timeToLiveSeconds="600">
		<persistence strategy="localTempSwap" />
	</cache>
	
	<cache name="estadoPorId" 
		maxEntriesLocalHeap="100"
		maxEntriesLocalDisk="1000" 
		eternal="false" 
		timeToIdleSeconds="300" 
		timeToLiveSeconds="600">
		<persistence strategy="localTempSwap" />
	</cache>
    
    <cache name="tiposDocumento" 
		maxEntriesLocalHeap="100"
		maxEntriesLocalDisk="1000" 
		eternal="false" 
		timeToIdleSeconds="300" 
		timeToLiveSeconds="600">
		<persistence strategy="localTempSwap" />
	</cache>
	
	<cache name="generos" 
		maxEntriesLocalHeap="100"
		maxEntriesLocalDisk="1000" 
		eternal="false" 
		timeToIdleSeconds="300" 
		timeToLiveSeconds="600">
		<persistence strategy="localTempSwap" />
	</cache>

	<cache name="escolaridades" 
		maxEntriesLocalHeap="100"
		maxEntriesLocalDisk="1000" 
		eternal="false" 
		timeToIdleSeconds="300" 
		timeToLiveSeconds="600">
		<persistence strategy="localTempSwap" />
	</cache>
	
	<cache name="eps" 
		maxEntriesLocalHeap="100"
		maxEntriesLocalDisk="1000" 
		eternal="false" 
		timeToIdleSeconds="300" 
		timeToLiveSeconds="600">
		<persistence strategy="localTempSwap" />
	</cache>

	<cache name="parentescos" 
		maxEntriesLocalHeap="100"
		maxEntriesLocalDisk="1000" 
		eternal="false" 
		timeToIdleSeconds="300" 
		timeToLiveSeconds="600">
		<persistence strategy="localTempSwap" />
	</cache>
	
	<cache name="parentescoPorId" 
